<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.praciceapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PraciceApplication"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".AuthenticationActivity"
            android:label="@string/title_activity_authentication"
            android:theme="@style/Theme.PraciceApplication.NoActionBar"></activity>
        <activity android:name=".ThirdActivity" />
        <activity android:name=".SecondActivity" /> <!-- first activity==MainActivity -->
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.PraciceApplication.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>